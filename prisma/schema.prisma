generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  name     String
  email    String  @unique()
  password String
}

model Product {
  id             Int      @id @default(autoincrement())
  operationDate  DateTime @default(now())
  symbol         String
  price          Float
  qnt            Int
  ownerId        Float
}